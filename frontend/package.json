{
  "name": "frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "electron": {
    "esm": true
  },
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "lint": "eslint .",
    "preview": "webpack serve --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:navigation": "jest src/__tests__/navigation/",
    "test:navigation:watch": "jest --watch src/__tests__/navigation/",
    "test:accessibility": "jest --testNamePattern=accessibility",
    "test:performance": "jest --testNamePattern=performance",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:navigation": "cypress run --spec cypress/e2e/navigation-*.cy.ts",
    "e2e": "start-server-and-test preview http://localhost:3000 cypress:run:navigation",
    "e2e:open": "start-server-and-test preview http://localhost:3000 cypress:open",
    "deploy": "node deploy.js",
    "clean-debug": "node clean-debug.js",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:3000 && cross-env DEV_SERVER_URL=http://localhost:3000 electron .\"",
    "build:prod": "cross-env NODE_ENV=production webpack --mode production",
    "electron:build": "npm run build:prod && electron-builder build --mac --win",
    "electron:build:mac": "npm run build:prod && electron-builder build --mac",
    "electron:build:win": "npm run build:prod && electron-builder build --win",
    "electron:dist": "npm run build:prod && electron-builder build --mac --win --publish=never",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "format:check": "prettier --check src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "@dataconnect/generated": "file:src/dataconnect-generated",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@electron/remote": "^2.1.3",
    "@nivo/bar": "^0.99.0",
    "@nivo/boxplot": "^0.99.0",
    "@nivo/core": "^0.99.0",
    "@nivo/heatmap": "^0.99.0",
    "@nivo/line": "^0.99.0",
    "@nivo/pie": "^0.99.0",
    "@types/react-router-dom": "^5.3.3",
    "canvas": "^3.2.0",
    "compromise": "^14.14.4",
    "core-js": "^3.37.0",
    "d3": "^7.9.0",
    "electron": "^38.1.0",
    "electron-builder": "^26.0.12",
    "electron-is-dev": "^3.0.1",
    "electron-store": "^11.0.0",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "firebase": "^12.3.0",
    "html2canvas": "^1.4.1",
    "html2canvas-pro": "^1.5.11",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.544.0",
    "ml-matrix": "^6.12.1",
    "ml-regression": "^6.3.0",
    "papaparse": "^5.5.3",
    "pptxgenjs": "^4.0.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
    "react-is": "^19.2.0",
    "react-router-dom": "^7.9.1",
    "react-share": "^5.2.2",
    "recharts": "^3.2.1",
    "regression": "^2.0.1",
    "simple-statistics": "^7.8.8",
    "uuid": "^13.0.0",
    "wait-on": "^8.0.5"
